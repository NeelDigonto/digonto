{
  "name": "gateway",
  "version": "1.0.0",
  "description": "api gateway",
  "type": "commonjs",
  "engines": {
    "node": ">= 20 <21"
  },
  "scripts": {
    "format:check": "prettier --config .prettierrc --check .",
    "format:write": "prettier --config .prettierrc --write .",
    "lint:check": "eslint . --ext .ts --ext .mts",
    "lint:fix": "eslint --fix . --ext .ts --ext .mts",
    "coverage": "dotenv-cli jest --coverage",
    "test": "NODE_ENV=test jest --verbose true --detectOpenHandles",
    "build": "swc src --out-dir dist",
    "start": "NODE_ENV=production node dist/main.js",
    "dev:compile": "swc src -w --out-dir dist",
    "dev:test": "NODE_ENV=test jest --watchAll --verbose true --detectOpenHandles",
    "dev:run": "nodemon --watch \"dist/**/*\" -e js  ./dist/main.js",
    "dev": "concurrently --raw 'npm run dev:compile' 'npm run dev:run'",
    "generate": "drizzle-kit generate:pg",
    "migrate-post": "node ./dist/db/migrate.js",
    "migrate": "npm run build && npm run migrate-post"
  },
  "author": "Saikat Dey",
  "license": "Unlicense",
  "dependencies": {
    "@aws-sdk/client-s3": "3.496.0",
    "@aws-sdk/s3-presigned-post": "3.496.0",
    "@aws-sdk/s3-request-presigner": "3.496.0",
    "@mikro-orm/core": "6.0.5",
    "@mikro-orm/postgresql": "6.0.5",
    "@mikro-orm/reflection": "6.0.5",
    "bignumber.js": "9.1.2",
    "date-fns": "3.3.0",
    "dotenv": "16.3.2",
    "express": "^4.18.2",
    "http-status-codes": "2.3.0",
    "nanoid": "5.0.4",
    "node-rdkafka": "^2.18.0",
    "postgres": "3.4.3",
    "reflect-metadata": "^0.2.1",
    "retry": "0.13.1",
    "winston": "3.11.0",
    "ws": "8.16.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.3.1",
    "@mikro-orm/cli": "6.0.5",
    "@mikro-orm/migrations": "6.0.5",
    "@mikro-orm/seeder": "6.0.5",
    "@swc-node/jest": "1.6.8",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.104",
    "@swc/jest": "0.2.31",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/retry": "0.12.5",
    "@types/uuid": "9.0.7",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "chokidar": "3.5.3",
    "concurrently": "8.2.2",
    "dotenv-cli": "7.3.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "27.6.3",
    "jest": "29.7.0",
    "nodemon": "3.0.3",
    "prettier": "2.8.8",
    "ts-essentials": "9.4.1",
    "tslib": "^2.6.2"
  }
}
